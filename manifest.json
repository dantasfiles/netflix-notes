{
  "manifest_version": 2,

  "name": "Netflix Notes",
  "description": "Lets you add notes to your Netflix queue entries",
  "version": "2.0",

  "content_scripts": [
    {
      "matches": ["http://*.netflix.com/Queue*"],
      "js": ["contentscript.js"]
    }
  ],

  "permissions": ["storage"]
}
